BITABLE_APP_TOKEN=bascnxxxxxxxxxxxxxxxxxxxxxx
BITABLE_TABLE_EMPLOYEES=tblxxxxxxxxxxxxxx
BITABLE_TABLE_OBJECTIVES=tblxxxxxxxxxxxxxx
BITABLE_TABLE_COMPLETIONS=tblxxxxxxxxxxxxxx
